<% if object.errors.any? %>
  <div id="error_explanation">
    <div class="alert alert-error">
      Исправьте, пожалуйста, следующие ошибки:    </div>
    <ul>
    <% object.errors.full_messages.each do |msg| %>
		<% if !msg.include? "content type" %>
			<li>* 
				<%=	msg %>
			</li>
		<% end %>		
    <% end %>
    </ul>
  </div>
<% end %>
